{
  "name": "payidshop",
  "version": "2.1.7",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "patch": "node patch.js",
    "prod": "ng build --prod",
    "deploy": "npx firebase deploy",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/stats-es2015.json",
    "ngrok": "ngrok http 4200 -host-header='localhost:4200'",
    "dev:ssr": "ng run payidshop:serve-ssr",
    "serve:ssr": "node dist/payidshop/server/main.js",
    "build:ssr": "ng build --prod && ng run payidshop:server:production",
    "prerender": "ng run payidshop:prerender",
    "deploy-gp": "npx deploy-gh --base-href=/pay-id-shop/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": {
    "name": "Paul Spread",
    "email": "spread2009@gmail.com"
  },
